module board_sprite(
	input         Clk,                // 50 MHz clock
                 Reset,              // Active-high reset signal
                 frame_clk,          // The clock indicating a new frame (~60Hz)
   input [9:0]   DrawX, DrawY,       // Current pixel coordinates
	input logic [15:0]  keycode,
   output logic  is_ball  
);


endmodule

